		},
		"transformIgnorePatterns": [
			"node_modules/(?!variables/.*)"
		]
	},
	"scripts": {
		"start:app": "PORT=3000 react-scripts start",
		"start:server": "NODE_ENV=development nodemon --config \"./utils/nodemon.json\"",
		"start": "NODE_ENV=development concurrently --kill-others \"yarn start:server\" \"yarn start:app\"",
		"build": "NODE_ENV=production . ./utils/build.sh",
		"serve": "node -r dotenv/config dist/server/index.js",
		"lint": "eslint \"./**/*.{ts,tsx,js}\" && prettier -c \"./**/*.{ts,tsx,js,json}\"",
